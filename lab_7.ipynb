    "def get_triples(n):\n",
    "    return [(x, y, z) for z in xrange(1, n) for y in range(1, z) for x in range(1, y) \n",
    "                         if x ** 2 + y ** 2 == z ** 2]\n",
    "\n",
    "\n",
    "print get_triples(22)\n",
    "print get_triples(33)"
   ]
  }
